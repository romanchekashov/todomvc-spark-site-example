{{#partial "styles"}}
    <link rel="stylesheet" href="js/node_modules/todomvc-common/base.css">
    <link rel="stylesheet" href="styles/index.css">
{{/partial}}

{{#partial "content" }}
    <section class="todoapp"></section>
{{/partial}}

{{#partial "js"}}
    <script src="js/node_modules/todomvc-common/base.js"></script>
    <script src="js/node_modules/react/dist/react-with-addons.js"></script>
    <script src="js/node_modules/classnames/index.js"></script>
    <script src="js/node_modules/react/dist/JSXTransformer.js"></script>
    <script src="js/node_modules/director/build/director.js"></script>

    <script src="js/utils.js"></script>
    <script src="js/todoModel.js"></script>
    <!-- jsx is an optional syntactic sugar that transforms methods in React's
    `render` into an HTML-looking format. Since the two models above are
    unrelated to React, we didn't need those transforms. -->
    <script type="text/jsx" src="js/todoItem.jsx"></script>
    <script type="text/jsx" src="js/footer.jsx"></script>
    <script type="text/jsx" src="js/app.jsx"></script>
{{/partial}}

{{> main.hbs}}
